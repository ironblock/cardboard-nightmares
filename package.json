{
  "name": "cardboard-nightmares",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cache:clean": "rm -rf ./cache",
    "cache:MTGJSON": "node utilities/cacheMTGJSON.mjs",
    "cache:Keyrune": "node utilities/convertKeyrune.mjs",
    "cache": "npm run cache:MTGJSON && npm run cache:Keyrune",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "types:check": "tsc --pretty --noEmit",
    "storybook:dev": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "prepare": "husky install && npm run cache:data",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "keyrune": "andrewgioia/keyrune#semver:^3.8.3",
    "ky": "^0.28.5",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reakit": "^1.3.9",
    "typescript": "4.4.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/builder-webpack5": "^6.3.8",
    "@storybook/manager-webpack5": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "@svgr/cli": "^5.5.0",
    "@svgr/core": "^5.5.0",
    "@svgr/plugin-jsx": "^5.5.0",
    "@svgr/plugin-prettier": "^5.5.0",
    "@svgr/plugin-svgo": "^5.5.0",
    "@types/react": "17.0.19",
    "cli-progress": "^3.9.1",
    "dotenv-webpack": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "gauge": "^3.0.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.0",
    "terser-webpack-plugin": "^5.2.4",
    "type-fest": "^2.3.3",
    "webpack": "^5.53.0"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7",
    "yarn": "YARN IS NOT SUPPORTED - PLEASE INSTALL NPM 7"
  }
}
